###
### Installation settings
###

# Program name
app.name=Seismic-Record Manager
app.jar=srm

# OS-specific stuff
ostask.unix-script.label=Install shortcut in:

# Each component must have a name, a size, and a 'fileset'. The fileset is the
# name of a resource, relative to the root of the JAR, where the list of files
# is stored, newline delimited
comp.count=6

comp.0.name=Seismic-Record Manager program (check only on initial install)
comp.0.disk-size=5
comp.0.real-size=4732
comp.0.fileset=program-srm

comp.1.name=California records
comp.1.disk-size=61
comp.1.real-size=59892
comp.1.fileset=eq-california
comp.1.sql=eq-california.sql
comp.1.sqllen=1223

comp.2.name=Chi-Chi, Taiwan earthquake records
comp.2.disk-size=113
comp.2.real-size=114234
comp.2.fileset=eq-chichi
comp.2.sql=eq-chichi.sql
comp.2.sqllen=630

comp.3.name=International records
comp.3.disk-size=9
comp.3.real-size=8616
comp.3.fileset=eq-international
comp.3.sql=eq-international.sql
comp.3.sqllen=115

comp.4.name=Other US records
comp.4.disk-size=38
comp.4.real-size=38139
comp.4.fileset=eq-otherus
comp.4.sql=eq-otherus.sql
comp.4.sqllen=165

comp.5.name=Source code
comp.5.disk-size=22
comp.5.real-size=13094
comp.5.fileset=program-source
